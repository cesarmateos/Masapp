<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:backgroundTint="#FF9800">

    <Button
        android:id="@+id/botCargaE1"
        android:layout_width="80dp"
        android:layout_height="80dp"
        android:layout_marginTop="0dp"
        android:backgroundTint="#9C7D73"
        android:fontFamily="monospace"
        android:text="@string/cargar_formato_predespacho"
        android:textColor="#F8F8F8"
        android:textSize="12sp"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/DesBotArriba"
        tools:ignore="MissingConstraints" />

    <ImageButton
        android:id="@+id/DesBotArriba"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="0dp"
        android:background="@drawable/boton_redondeado"
        android:contentDescription="Subir"
        android:gravity="center"
        android:rotation="270"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/ic_flechamia"
        tools:ignore="MissingConstraints" />


    <ImageButton
        android:id="@+id/DesBotAbajo"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_gravity="center_horizontal"
        android:layout_marginTop="0dp"
        android:background="@drawable/boton_redondeado"
        android:contentDescription="Bajar"
        android:rotation="90"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/botCargaE1"
        app:srcCompat="@drawable/ic_flechamia"
        tools:ignore="MissingConstraints" />


    <ImageButton
        android:id="@+id/DesBotIzq"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="70dp"
        android:layout_marginEnd="10dp"
        android:background="@drawable/boton_redondeado"
        android:contentDescription="Mover Izquierda"
        android:rotation="180"
        app:layout_constraintEnd_toStartOf="@+id/botCargaE1"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/ic_flechamia"
        tools:ignore="MissingConstraints" />

    <ImageButton
        android:id="@+id/DesBotDer"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="10dp"
        android:layout_marginTop="70dp"
        android:background="@drawable/boton_redondeado"
        android:contentDescription="Mover Derecha"
        app:layout_constraintStart_toEndOf="@+id/botCargaE1"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/ic_flechamia"
        tools:ignore="MissingConstraints" />

    <TextView
        android:id="@+id/DesEjeYText"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="32dp"
        android:layout_marginTop="24dp"
        android:text="@string/horizontal"
        android:textColor="#514D46"
        android:textSize="24sp"
        android:textStyle="bold"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/DesBotAbajo"
        tools:ignore="MissingConstraints" />

    <TextView
        android:id="@+id/DesTextoY"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="16dp"
        android:layout_marginTop="24dp"
        android:text="TextView"
        android:textColor="#514D46"
        android:textSize="24sp"
        app:layout_constraintStart_toEndOf="@+id/DesEjeYText"
        app:layout_constraintTop_toBottomOf="@+id/DesBotAbajo"
        tools:ignore="MissingConstraints" />

    <TextView
        android:id="@+id/DesTextoX"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="2dp"
        android:text="TextView"
        android:textColor="#514D46"
        android:textSize="24sp"
        app:layout_constraintStart_toStartOf="@+id/DesTextoY"
        app:layout_constraintTop_toBottomOf="@+id/DesTextoY"
        tools:ignore="MissingConstraints" />

    <TextView
        android:id="@+id/DesEjeXText"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="32dp"
        android:layout_marginTop="2dp"
        android:text="@string/vertical"
        android:textColor="#514D46"
        android:textSize="24sp"
        android:textStyle="bold"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/DesEjeYText"
        tools:ignore="MissingConstraints" />


    <Button
        android:id="@+id/botImprimePack"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginStart="24dp"
        android:layout_marginTop="24dp"
        android:layout_marginEnd="24dp"
        android:backgroundTint="#9C7D73"
        android:textColor="#F8F8F8"
        android:padding="8dp"
        android:text="@string/imprimir_prueba_pack"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/DesTextoX"
        tools:ignore="MissingConstraints" />

    <Button
        android:id="@+id/botImprimeIOMA"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginStart="24dp"
        android:layout_marginTop="2dp"
        android:layout_marginEnd="24dp"
        android:backgroundTint="#9C7D73"
        android:textColor="#F8F8F8"
        android:padding="8dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/botImprimePack"
        android:text="@string/imprimir_prueba_ioma"
        tools:ignore="MissingConstraints" />

    <Button
        android:id="@+id/botImprimeRef"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginStart="24dp"
        android:layout_marginTop="2dp"
        android:layout_marginEnd="24dp"
        android:backgroundTint="#9C7D73"
        android:textColor="#F8F8F8"
        android:padding="8dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/botImprimeIOMA"
        android:text="@string/imprimir_prueba_refrigerado"
        tools:ignore="MissingConstraints" />


</androidx.constraintlayout.widget.ConstraintLayout>